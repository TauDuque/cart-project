n      value={{\n        ...state,\n        clearCart,\n        remove,\n        increase,\n        decrease,\n        toggleAmount,\n      }}\n    >\n      {children}\n    </AppContext.Provider>\n  );\n};\n// make sure use\nexport const useGlobalContext = () => {\n  return useContext(AppContext);\n};\n\nexport { AppContext, AppProvider };\n","/home/tau/react projects1001/react-projects/14-cart/setup/cart-project/src/App.js",[],"/home/tau/react projects1001/react-projects/14-cart/setup/cart-project/src/data.js",["41"],"export default [\n  {\n    id: 1,\n    title: 'Samsung Galaxy S7',\n    price: 599.99,\n    img:\n      'https://res.cloudinary.com/diqqf3eq2/image/upload/v1583368215/phone-2_ohtt5s.png',\n    amount: 1,\n  },\n  {\n    id: 2,\n    title: 'google pixel ',\n    price: 499.99,\n    img:\n      'https://res.cloudinary.com/diqqf3eq2/image/upload/v1583371867/phone-1_gvesln.png',\n    amount: 1,\n  },\n  {\n    id: 3,\n    title: 'Xiaomi Redmi Note 2',\n    price: 699.99,\n    img:\n      'https://res.cloudinary.com/diqqf3eq2/image/upload/v1583368224/phone-3_h2s6fo.png',\n    amount: 1,\n  },\n];\n","/home/tau/react projects1001/react-projects/14-cart/setup/cart-project/src/reducer.js",[],"/home/tau/react projects1001/react-projects/14-cart/setup/cart-project/src/CartContainer.js",[],"/home/tau/react projects1001/react-projects/14-cart/setup/cart-project/src/Navbar.js",[],"/home/tau/react projects1001/react-projects/14-cart/setup/cart-project/src/CartItem.js",["42","43"],"import React from \"react\";\nimport { useGlobalContext } from \"./context\";\nconst CartItem = ({ id, img, title, price, amount }) => {\n  const { remove, increase, decrease, toggleAmount } = useGlobalContext();\n  return (\n    <article className=\"cart-item\">\n      <img src={img} alt={title} />\n      <div>\n        <h4>{title}</h4>\n        <h4 className=\"item-price\">${price}</h4>\n        {/* remove button */}\n        <button className=\"remove-btn\" onClick={() => remove(id)}>\n          remove\n        </button>\n      </div>\n      <div>\n        {/* increase amount */}\n        <button className=\"amount-btn\" onClick={() => toggleAmount(id, \"inc\")}>\n          <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 20 20\">\n            <path d=\"M10.707 7.05L10 6.343 4.343 12l1.414 1.414L10 9.172l4.243 4.242L15.657 12z\" />\n          </svg>\n        </button>\n        {/* amount */}\n        <p className=\"amount\">{amount}</p>\n        {/* decrease amount */}\n        <button className=\"amount-btn\" onClick={() => toggleAmount(id, \"dec\")}>\n          <svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 20 20\">\n            <path d=\"M9.293 12.95l.707.707L15.657 8l-1.414-1.414L10 10.828 5.757 6.586 4.343 8z\" />\n          </svg>\n        </button>\n      </div>\n    </article>\n  );\n};\n\nexport default CartItem;\n",{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":1,"column":17,"nodeType":"50","messageId":"51","endLine":1,"endColumn":25},{"ruleId":"52","severity":1,"message":"53","line":1,"column":1,"nodeType":"54","endLine":26,"endColumn":3},{"ruleId":"48","severity":1,"message":"55","line":4,"column":19,"nodeType":"50","messageId":"51","endLine":4,"endColumn":27},{"ruleId":"48","severity":1,"message":"56","line":4,"column":29,"nodeType":"50","messageId":"51","endLine":4,"endColumn":37},"no-native-reassign",["57"],"no-negated-in-lhs",["58"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign array to a variable before exporting as module default","ExportDefaultDeclaration","'increase' is assigned a value but never used.","'decrease' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]